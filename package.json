{
  "name": "throttle-repeat",
  "version": "2.1.2",
  "description": "Repeatedly executes a given task at a given maximum rate (in milliseconds) until a given condition is true",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "coverage": "nyc report --reporter=lcov",
    "coveralls-coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint .",
    "test": "nyc --all mocha --recursive ./test",
    "posttest": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexanderMS/throttle-repeat.git"
  },
  "keywords": [
    "throttle",
    "rate",
    "task",
    "promise",
    "repeat",
    "until",
    "reduce",
    "milliseconds",
    "ms"
  ],
  "author": "Alexandr Murashkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexanderMS/throttle-repeat/issues"
  },
  "homepage": "https://github.com/AlexanderMS/throttle-repeat#readme",
  "dependencies": {
    "co": "4.6.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.13.0",
    "eslint": "^3.12.0",
    "mocha": "^3.2.0",
    "nyc": "^10.2.0"
  }
}
